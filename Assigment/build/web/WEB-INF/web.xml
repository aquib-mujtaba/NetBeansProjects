<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Profiler</servlet-name>
        <servlet-class>assig.register_form.Profiler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Visiter</servlet-name>
        <servlet-class>visitor.Visiter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StartSesseion</servlet-name>
        <servlet-class>sespack.StartSesseion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileDisplay</servlet-name>
        <servlet-class>assig.ProfileDisplay</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateForm</servlet-name>
        <servlet-class>UpdateForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdaterForm</servlet-name>
        <servlet-class>assign.update.UpdaterForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logger</servlet-name>
        <servlet-class>assig.login.Logger</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>assig.delete.Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Profiler</servlet-name>
        <url-pattern>/Profiler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileDisplay</servlet-name>
        <url-pattern>/ProfileDisplay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateForm</servlet-name>
        <url-pattern>/UpdateForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdaterForm</servlet-name>
        <url-pattern>/UpdaterForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logger</servlet-name>
        <url-pattern>/Logger</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/Delete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
