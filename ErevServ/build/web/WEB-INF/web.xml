        <servlet-name>Deleter</servlet-name>
        <url-pattern>/Deleter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateForm</servlet-name>
        <url-pattern>/UpdateForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdaterForm</servlet-name>
        <url-pattern>/UpdaterForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logger</servlet-name>
        <url-pattern>/Logger</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
