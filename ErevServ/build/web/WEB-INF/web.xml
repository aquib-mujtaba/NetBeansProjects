<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SimpleServ</servlet-name>
        <servlet-class>startserv.SimpleServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UrlParam</servlet-name>
        <servlet-class>startserv.UrlParam</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InitPram</servlet-name>
        <servlet-class>startserv.InitPram</servlet-class>
        <init-param>
            <param-name>FirstName</param-name>
            <param-value>Aquib</param-value>
        </init-param>
        <init-param>
            <param-name>lastName</param-name>
            <param-value>Mujtaba</param-value>
        </init-param>
        <init-param>
            <param-name>Subject</param-name>
            <param-value>Java</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Profiler</servlet-name>
        <servlet-class>projpack.Profiler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Visiter</servlet-name>
        <servlet-class>startserv.Visiter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StartSesseion</servlet-name>
        <servlet-class>sespack.StartSesseion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Processer</servlet-name>
        <servlet-class>sespack.Processer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Retreive</servlet-name>
        <servlet-class>sespack.Retreive</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileDisplay</servlet-name>
        <servlet-class>assig.ProfileDisplay</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Report</servlet-name>
        <servlet-class>assig.Report</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>assig.Delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Deleter</servlet-name>
        <servlet-class>assig.Deleter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateForm</servlet-name>
        <servlet-class>UpdateForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdaterForm</servlet-name>
        <servlet-class>assign.update.UpdaterForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logger</servlet-name>
        <servlet-class>assig.login.Logger</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SimpleServ</servlet-name>
        <url-pattern>/SimpleServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UrlParam</servlet-name>
        <url-pattern>/UrlParam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InitPram</servlet-name>
        <url-pattern>/InitPram</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Profiler</servlet-name>
        <url-pattern>/Profiler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Visiter</servlet-name>
        <url-pattern>/Visiter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StartSesseion</servlet-name>
        <url-pattern>/StartSesseion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Processer</servlet-name>
        <url-pattern>/Processer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Retreive</servlet-name>
        <url-pattern>/Retreive</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileDisplay</servlet-name>
        <url-pattern>/ProfileDisplay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Report</servlet-name>
        <url-pattern>/Report</url-pattern>
    </servlet-mapping>
